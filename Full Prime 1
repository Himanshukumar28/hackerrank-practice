import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner sc = new Scanner(System.in);
        long n = sc.nextInt();
        boolean check = true;
        while(n>0){
            long rem = n%10;
            if(rem!=2 & rem!= 3 & rem!=5 & rem!=7){
                check = false;
                break;
            }
            n = n/10;
        }
        System.out.print(check);
    }
}
